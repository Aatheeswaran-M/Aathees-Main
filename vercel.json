{
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/agro",
      "destination": "https://agro-mate-nine.vercel.app"
    },
    {
      "source": "/agro/(.*)",
      "destination": "https://agro-mate-nine.vercel.app/$1"
    },
    {
      "source": "/weather/(.*)",
      "destination": "https://findweather.vercel.app/$1"
    },
    {
      "source": "/news/(.*)",
      "destination": "https://news-app-mpx1.vercel.app/$1"
    },
    {
      "source": "/bio/(.*)",
      "destination": "https://your-bio.vercel.app/$1"
    },
    {
      "source": "/(.*)",
      "destination": "https://aathees-main.vercel.app/$1"
    }
  ]
}
