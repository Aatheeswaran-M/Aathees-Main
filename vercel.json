{
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/weather/assets/:match*",
      "destination": "https://findweather.vercel.app/assets/:match*"
    },
    {
      "source": "/weather/:match*",
      "destination": "https://findweather.vercel.app/:match*"
    },
    {
      "source": "/agro/:match*",
      "destination": "https://agro-mate-nine.vercel.app/:match*"
    },
    {
      "source": "/pharma/:match*",
      "destination": "https://pharmahub.vercel.app/:match*"
    },
    {
      "source": "/apps/sms-drive/:match*",
      "destination": "https://sms-drive-website.vercel.app/:match*"
    },
    {
      "source": "/blog/:match*",
      "destination": "https://your-blog.vercel.app/:match*"
    },
    {
      "source": "/bio/:match*",
      "destination": "https://your-bio.vercel.app/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "https://portfolio-frontend-seven-pi.vercel.app/$1"
    }
  ]
}
