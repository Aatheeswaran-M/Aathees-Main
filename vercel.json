{
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/agro",
      "destination": "https://agro-mate-nine.vercel.app"
    },
    {
      "source": "/agro/(.*)",
      "destination": "https://agro-mate-nine.vercel.app/$1"
    },
    {
      "source": "/weather/(.*)",
      "destination": "https://findweather.vercel.app/$1"
    },
     {
      "source": "/news",
      "destination": "https://news-app-mpxl.vercel.app"
    },
    {
      "source": "/news/(.*)",
      "destination": "https://news-app-mpxl.vercel.app/$1"
    },
     {
      "source": "/resume",
      "destination": "https://aathees-resume.vercel.app"
    },
    {
      "source": "/resume/(.*)",
      "destination": "https://aathees-resume.vercel.app/$1"
    },
    {
      "source": "/bio/(.*)",
      "destination": "https://your-bio.vercel.app/$1"
    },
    {
      "source": "/(.*)",
      "destination": "https://portfolio-frontend-seven-pi.vercel.app/$1"
    }
  ]
}
